{
  "version": "2.0.0",
  "tasks": [
    {
      "type": "cppbuild",
      "label": "C/C++: cl.exe build active file",
      "command": "cl.exe",
      "args": [
        "/std:c++20",
        "/Zi",
        "/EHsc",
        "/nologo",
        "/FI${workspaceFolder}\\Sources\\Core\\CoreAPI.h",
        "/Fo${workspaceFolder}\\Intermediate\\Object\\${fileBasenameNoExtension}.obj",
        "/Fe${workspaceFolder}\\Execution\\${fileBasenameNoExtension}.exe",
        "${file}"
      ],
      "options": {
        "cwd": "${fileDirname}"
      },
      "problemMatcher": [
        "$msCompile"
      ],
      "group": {
        "kind": "build",
        "isDefault": false
      },
      "detail": "Task generated by Debugger."
    }
  ]
}